<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="apsCsszTempCode"
		title="{i18n>apsCsszTempCodeTitle}"
		class="sapUiPopupWithPadding"
		items="{ct>/}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			title="{ct>siteCode} {ct>templateText}"
			description="{ct>templateCode}" />
	</SelectDialog>
</core:FragmentDefinition>